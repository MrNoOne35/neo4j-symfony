{
    "name": "mrnoone35/neo4j-bundle",
    "description": "Symfony integration for Neo4j",
    "type": "symfony-bundle",
    "keywords": ["neo4j"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        },
        {
            "name": "Xavier Coureau",
            "email": "xavier@pandawan-technology.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "anyitsolutions/neo4j-bolt": "^1.6",
        "laudis/neo4j-php-client": "^2.1",
        "symfony/dependency-injection": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-kernel": "^5.3",
        "twig/twig": "^2.0 || ^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1"
    },
    "suggest": {
        "plumtreesystems/neo4j-php-ogm": "To have EntityManager support"
    },
    "autoload": {
        "psr-4": {
            "Neo4j\\Neo4jBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit",
        "test-ci": "vendor/bin/simple-phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
